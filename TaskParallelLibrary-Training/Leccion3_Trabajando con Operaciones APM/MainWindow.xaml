<Window x:Class="Leccion3_Trabajando_con_Operaciones_APM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Leccion3_Trabajando_con_Operaciones_APM"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Name="tbURL" Height="38"></TextBox>
            <Button Name="btnValidarURL" Width="100" Height="38" Click="btnValidarURL_Click">Validar URL</Button>
            <Label Name="lblResultado" Height="38"></Label>
        </StackPanel>
    </Grid>
</Window>
